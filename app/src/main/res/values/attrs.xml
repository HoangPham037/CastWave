<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="UnderlineText">
        <attr name="isUnderline" format="boolean" />
    </declare-styleable>
    <declare-styleable name="CustomProgressBar">
        <attr name="mStrokeWidth" format="dimension"/>
        <attr name="mAnimationDuration" format="integer"/>
        <attr name="mMaxProgress" format="integer"/>
        <attr name="mProgressColor" format="color"/>
    </declare-styleable>


    <declare-styleable name="OtpTextView">
        <attr name="otp" format="string" />
        <attr name="width" format="dimension" />
        <attr name="height" format="dimension" />
    </declare-styleable>
</resources>